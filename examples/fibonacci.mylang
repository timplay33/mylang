// Simple Fibonacci example
func int fib(int n) {
    if (n <= 1) {
        return n;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

print("Fibonacci Numbers:");
print("fib(0) =", fib(0));
print("fib(1) =", fib(1));
print("fib(2) =", fib(2));
print("fib(3) =", fib(3));
print("fib(4) =", fib(4));
print("fib(5) =", fib(5));
