// Calculator example demonstrating arithmetic operations
int a = 10;
int b = 3;

print("Calculator Demo");
print("a =", a);
print("b =", b);
print("a + b =", a + b);
print("a - b =", a - b);
print("a * b =", a * b);
print("a / b =", a / b);

